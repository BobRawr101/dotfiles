[color]
    pager       = true
    ui          = auto
    status      = auto
    diff        = auto
    branch      = auto
    showBranch  = auto
    interactive = auto
    grep        = auto
[color "status"]
    header    = black bold
    branch    = magenta bold
    nobranch  = red
    unmerged  = red
    untracked = red
    added     = green bold
    changed   = red bold
[color "diff"]
    meta       = black bold
    frag       = black bold
    func       = cyan
    old        = red
    new        = green
    commit     = yellow
    whitespace = red
    context    = normal
[color "branch"]
    current  = cyan bold
    local    = cyan
    remote   = magenta
    upstream = magenta
    plain    = normal
[color "decorate"]
    branch = green bold
    remoteBranch = magenta bold
    tag = magenta
    stash = cyan
    HEAD = cyan bold
[color "interactive"]
    prompt = red
    header = red bold
    error = red
    help = black bold
[color "grep"]
    context = normal
    match = cyan
    filename = blue
    function = blue
    selected = normal
    separator = red bold
    linenumber = normal
